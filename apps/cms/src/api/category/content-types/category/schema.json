{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": "Scooter categories for filtering"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "displayName": {
      "type": "string",
      "required": true
    },
    "icon": {
      "type": "string"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "scooters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::scooter.scooter",
      "mappedBy": "categories"
    }
  }
}

